# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.Application.index

GET     /nodes/:id                  controllers.Application.getNode(id: ObjectId)
GET     /nodes/                     controllers.Application.nodes
GET     /nodes                      controllers.Application.nodes
POST    /nodes                      controllers.Application.newNode
DELETE  /nodes/:id                  controllers.Application.deleteNode(id:ObjectId)
PUT     /nodes/:id                  controllers.Application.updateNode(id:ObjectId)
