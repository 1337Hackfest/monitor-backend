# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


#GET     /nodes                      controllers.Application.listNodes
#PUT     /node/:id                   controllers.Application.addNode(id:Long)
#DELETE  /node/:id                   controllers.Application.deleteNode(id:Long)
#POST    /node/:id                   controllers.Application.updateNode(id:Long)

# Home page
GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET     /nodes                  controllers.Application.listNodes
POST    /node                  controllers.Application.addNode
POST    /node/:id/delete       controllers.Application.deleteNode(id: String)